{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS containing HMR, linting, and routing",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack",
    "build-dev": "webpack-dev-server --open",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "start": "npm-run-all --parallel build-dev lint:watch"
  },
  "author": "clarson469",
  "license": "MIT",
  "keywords": [
    "react",
    "webpack",
    "react-router",
    "react-hot-loader",
    "boilerplate"
  ],
  "repository": "https://github.com/clarson469/react-boilerplate",
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.2",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.10.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.3",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
